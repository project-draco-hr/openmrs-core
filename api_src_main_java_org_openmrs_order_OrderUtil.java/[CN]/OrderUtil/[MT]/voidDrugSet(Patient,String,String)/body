{
  if (log.isDebugEnabled())   log.debug("Voiding drug sets for patient: " + patient + " drugSetId: "+ drugSetId+ " reason: "+ voidReason);
  if (Context.isAuthenticated() == false)   throw new ContextAuthenticationException("Unable to void drugs when no one is logged in");
  if (patient == null)   throw new APIException("Unable to void drugs without a patient being given");
  if (drugSetId == null)   throw new APIException("Unable to void drugs without a drugSetId being given");
  OrderService orderService=Context.getOrderService();
}
