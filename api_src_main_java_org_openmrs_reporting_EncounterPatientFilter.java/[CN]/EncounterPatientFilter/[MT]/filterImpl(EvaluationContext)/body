{
  PatientSetService service=Context.getPatientSetService();
  return service.getPatientsHavingEncounters(encounterTypeList,location,form,OpenmrsUtil.fromDateHelper(null,withinLastDays,withinLastMonths,untilDaysAgo,untilMonthsAgo,sinceDate,untilDate),OpenmrsUtil.toDateHelper(null,withinLastDays,withinLastMonths,untilDaysAgo,untilMonthsAgo,sinceDate,untilDate),atLeastCount,atMostCount);
}
