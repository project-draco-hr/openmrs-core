{
  for (  Object o : list) {
    if (o instanceof Integer) {
      return true;
    }
 else     if (o instanceof PatientSearch) {
      return ((PatientSearch)o).requiresHistory();
    }
 else     if (o instanceof List && requiresHistoryHelper((List<Object>)o)) {
      return true;
    }
  }
  return false;
}
