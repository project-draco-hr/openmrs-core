{
  Set<Obs> MyObs;
  Obs obs=new Obs();
{
    MyObs=Context.getObsService().getObservations(patient,concept,true);
    Iterator iter=MyObs.iterator();
    if (iter.hasNext()) {
      while (iter.hasNext()) {
        obs=(Obs)iter.next();
      }
      return obs;
    }
 else {
      return null;
    }
  }
}
