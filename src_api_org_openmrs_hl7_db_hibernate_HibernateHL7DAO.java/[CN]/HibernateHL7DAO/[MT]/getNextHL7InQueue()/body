{
  Query query=sessionFactory.getCurrentSession().createQuery("from HL7InQueue as hiq where hiq.messageState = ? order by HL7InQueueId").setParameter(0,HL7Constants.HL7_STATUS_PENDING,Hibernate.INTEGER).setMaxResults(1);
  if (query == null)   return null;
  return (HL7InQueue)query.uniqueResult();
}
