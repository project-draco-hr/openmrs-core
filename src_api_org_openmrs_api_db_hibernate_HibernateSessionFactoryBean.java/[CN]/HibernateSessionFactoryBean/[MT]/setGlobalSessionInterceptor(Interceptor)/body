{
  log.debug("Spring setter for global Hibernate Session Interceptor for SessionFactory called with interceptor: " + globalSessionInterceptor);
  if (HibernateSessionFactoryBean.globalSessionInterceptor != null) {
    log.warn("Overwriting current interceptor: " + HibernateSessionFactoryBean.globalSessionInterceptor + " with another interceptor: "+ globalSessionInterceptor);
  }
  HibernateSessionFactoryBean.globalSessionInterceptor=globalSessionInterceptor;
}
