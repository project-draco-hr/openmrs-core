{
  List<Extension> extensions=ModuleFactory.getExtensions("org.openmrs.module.web.extension.AddEncounterToVisitExtension");
  if (extensions == null) {
    return Collections.emptySet();
  }
  Set<Form> forms=new TreeSet<Form>(new Comparator<Form>(){
    @Override public int compare(    Form o1,    Form o2){
      return o1.getName().compareTo(o2.getName());
    }
  }
);
  for (  Extension extension : extensions) {
    AddEncounterToVisitExtension ext=(AddEncounterToVisitExtension)extension;
    Set<Form> tmpForms=ext.getFormsModuleCanAddEncounterToVisit();
    if (tmpForms != null) {
      forms.addAll(tmpForms);
    }
  }
  return forms;
}
