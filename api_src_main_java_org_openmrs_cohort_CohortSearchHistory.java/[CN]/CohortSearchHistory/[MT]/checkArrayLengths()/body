{
  int n=searchHistory.size();
  while (cachedFilters.size() > n)   cachedFilters.remove(n);
  while (cachedResults.size() > n)   cachedResults.remove(n);
  while (cachedResultDates.size() > n)   cachedResultDates.remove(n);
  while (cachedFilters.size() < n)   cachedFilters.add(null);
  while (cachedResults.size() < n)   cachedResults.add(null);
  while (cachedResultDates.size() < n)   cachedResultDates.add(null);
}
