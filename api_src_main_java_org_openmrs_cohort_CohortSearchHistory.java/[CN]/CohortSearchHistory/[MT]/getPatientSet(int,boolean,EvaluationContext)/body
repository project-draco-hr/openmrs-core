{
  checkArrayLengths();
  Cohort ret=null;
synchronized (this) {
    if (useCache) {
      ret=cachedResults.get(i);
    }
    if (ret == null) {
      ensureCachedFilter(i);
      PatientFilter pf=cachedFilters.get(i);
      ret=pf.filter(null,context);
      cachedFilters.set(i,pf);
      cachedResults.set(i,ret);
      cachedResultDates.set(i,new Date());
    }
  }
  return ret;
}
