{
  List<Person> whom=new Vector<Person>();
  if (who != null && who.getPersonId() != null)   whom.add(who);
  List<Concept> questions=new Vector<Concept>();
  questions.add(question);
  return Context.getObsService().getObservations(whom,null,questions,null,null,null,null,null,null,null,null,false);
}
