{
  if (obs == null || obs.length < 1)   return;
  String conceptIdStr=Context.getAdministrationService().getGlobalProperty(OpenmrsConstants.GLOBAL_PROPERTY_MEDICAL_RECORD_OBSERVATIONS,"1238");
  if (conceptIdStr == null || conceptIdStr.length() == 0)   return;
  Integer conceptId=Integer.valueOf(conceptIdStr);
  Concept defaultObsGroupConcept=Context.getConceptService().getConcept(conceptId);
  if (defaultObsGroupConcept == null)   throw new APIException("There is no concept defined with concept id: " + conceptIdStr + "You should correctly define the default obs group concept id with the global propery"+ OpenmrsConstants.GLOBAL_PROPERTY_MEDICAL_RECORD_OBSERVATIONS);
  Obs obsGroup=new Obs();
  obsGroup.setConcept(defaultObsGroupConcept);
  for (  Obs member : obs) {
    obsGroup.addGroupMember(member);
  }
  updateObs(obsGroup);
}
