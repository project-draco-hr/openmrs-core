{
  if (obj == null)   return false;
  if (obj instanceof Concept) {
    Concept c=(Concept)obj;
    if (getConceptId() == null && c.getConceptId() == null)     return this == obj;
    if (getConceptId() != null)     return (this.getConceptId().equals(c.getConceptId()));
  }
  return this == obj;
}
