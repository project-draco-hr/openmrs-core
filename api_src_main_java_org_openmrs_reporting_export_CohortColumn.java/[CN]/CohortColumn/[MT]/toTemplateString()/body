{
  if (cohortId != null)   return "$!{fn.getCohortMembership(" + cohortId + ", '"+ valueIfTrue+ "', '"+ valueIfFalse+ "')}";
 else   if (filterId != null)   return "$!{fn.getCohortDefinitionMembership(" + filterId + ", '"+ valueIfTrue+ "', '"+ valueIfFalse+ "')}";
 else   return "$!{fn.getPatientSearchMembership(" + patientSearchId + ", '"+ valueIfTrue+ "', '"+ valueIfFalse+ "')}";
}
