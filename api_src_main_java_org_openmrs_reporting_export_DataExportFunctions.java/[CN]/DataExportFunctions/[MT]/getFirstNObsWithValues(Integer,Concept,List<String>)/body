{
  attrs.add(0,null);
  List<List<Object>> obs=getObsWithValues(concept,attrs);
  if (obs == null)   obs=new Vector<List<Object>>();
  if (n.equals(-1))   return obs;
  List<Object> blankRow=new Vector<Object>();
  for (  String attr : attrs)   blankRow.add("");
  while (obs.size() < n)   obs.add(0,blankRow);
  int size=obs.size();
  List<List<Object>> rList=obs.subList(size - n,size);
  Collections.reverse(rList);
  return rList;
}
