{
  List<AlertListItem> alerts=new Vector<AlertListItem>();
  try {
    AlertService as=Context.getAlertService();
    for (    Alert a : as.getAlertsByUser(null)) {
      alerts.add(new AlertListItem(a));
    }
  }
 catch (  Exception e) {
    log.error("Error getting alerts",e);
  }
  return alerts;
}
