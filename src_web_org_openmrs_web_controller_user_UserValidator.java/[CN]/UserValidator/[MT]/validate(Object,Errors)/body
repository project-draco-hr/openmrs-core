{
  User user=(User)obj;
  if (user == null) {
    errors.rejectValue("user","error.general");
  }
 else {
    if (user.getUserId() != null)     ValidationUtils.rejectIfEmptyOrWhitespace(errors,"systemId","error.systemId");
    if (user.isVoided() && user.getVoidReason().trim().equals(""))     errors.rejectValue("voidReason","error.null");
  }
}
