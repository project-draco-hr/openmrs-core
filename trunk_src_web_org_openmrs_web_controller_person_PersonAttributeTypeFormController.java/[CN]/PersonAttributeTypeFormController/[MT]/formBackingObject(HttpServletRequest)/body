{
  PersonAttributeType attrType=null;
  if (Context.isAuthenticated()) {
    PersonService ps=Context.getPersonService();
    String attrTypeId=request.getParameter("personAttributeTypeId");
    if (attrTypeId != null)     attrType=ps.getPersonAttributeType(Integer.valueOf(attrTypeId));
  }
  if (attrType == null)   attrType=new PersonAttributeType();
  return attrType;
}
