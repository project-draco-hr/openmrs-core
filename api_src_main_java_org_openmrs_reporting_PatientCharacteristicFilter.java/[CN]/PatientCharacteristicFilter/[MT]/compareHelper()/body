{
  int ret=0;
  if (deadOnly != null) {
    ret+=deadOnly ? 2 : 1;
  }
  if (aliveOnly != null) {
    ret+=aliveOnly ? 20 : 10;
  }
  if (minAge != null) {
    ret+=minAge * 100;
  }
  if (maxAge != null) {
    ret+=maxAge * 1000;
  }
  if (gender != null) {
    ret+=gender.equals("M") ? 1000000 : 2000000;
  }
  return ret;
}
