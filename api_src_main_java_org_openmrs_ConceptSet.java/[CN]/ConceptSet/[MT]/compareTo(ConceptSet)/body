{
  if ((getSortWeight() == null) && (cs.getSortWeight() != null))   return -1;
  if ((getSortWeight() != null) && (cs.getSortWeight() == null))   return 1;
  if ((getSortWeight() == null) && (cs.getSortWeight() == null))   return 0;
  return (getSortWeight() < cs.getSortWeight()) ? -1 : (getSortWeight() > cs.getSortWeight()) ? 1 : 0;
}
