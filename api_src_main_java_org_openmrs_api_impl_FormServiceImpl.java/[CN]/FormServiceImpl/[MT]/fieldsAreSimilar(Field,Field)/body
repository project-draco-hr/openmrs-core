{
  return (OpenmrsUtil.nullSafeEquals(field.getName(),fieldToBeReplaced.getName()) && OpenmrsUtil.nullSafeEquals(field.getSelectMultiple(),fieldToBeReplaced.getSelectMultiple()) && OpenmrsUtil.nullSafeEquals(field.getFieldType(),fieldToBeReplaced.getFieldType())&& OpenmrsUtil.nullSafeEquals(field.getConcept(),fieldToBeReplaced.getConcept())&& OpenmrsUtil.nullSafeEquals(field.getTableName(),fieldToBeReplaced.getTableName())&& OpenmrsUtil.nullSafeEquals(field.getDefaultValue(),fieldToBeReplaced.getDefaultValue())&& field.getRetired() != null && !field.getRetired());
}
