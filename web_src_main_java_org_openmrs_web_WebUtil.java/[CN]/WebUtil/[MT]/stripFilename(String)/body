{
  if (log.isDebugEnabled())   log.debug("Stripping filename from: " + filename);
  int index=filename.lastIndexOf("/");
  if (index != -1)   filename=filename.substring(index + 1);
  index=filename.lastIndexOf("\\");
  if (index != -1)   filename=filename.substring(index + 1);
  if (log.isDebugEnabled())   log.debug("Returning stripped down filename: " + filename);
  return filename;
}
