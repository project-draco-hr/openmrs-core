{
  if (attrs == null)   attrs=new Vector<String>();
  attrs.add(0,null);
  List<List<Object>> returnList=getObsWithValues(concept,attrs);
  if (returnList == null)   returnList=new Vector<List<Object>>();
  if (n.equals(-1))   return returnList;
  List<Object> blankRow=new Vector<Object>();
  for (  String attr : attrs)   blankRow.add("");
  while (returnList.size() < n)   returnList.add(blankRow);
  List<List<Object>> rList=returnList.subList(0,n);
  return rList;
}
