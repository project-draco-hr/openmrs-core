{
  StringBuilder s=new StringBuilder("");
  if (extras != null) {
    for (    String ext : extras) {
      s.append("$!{fn.getSeparator()}").append("\"").append(columnName).append("_").append(ext);
      if (appendCount) {
        s.append("_($velocityCount)");
      }
      s.append("\"");
    }
  }
  return s.toString();
}
