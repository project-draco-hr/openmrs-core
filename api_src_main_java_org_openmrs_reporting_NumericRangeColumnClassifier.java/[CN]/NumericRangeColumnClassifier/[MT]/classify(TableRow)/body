{
  Object o=row.get(columnName);
  if (o == null) {
    return missingLabel;
  }
  double value;
  if (o instanceof Number) {
    value=((Number)o).doubleValue();
  }
 else {
    value=Double.valueOf(o.toString());
  }
  if (o instanceof Number) {
    for (int i=0; i < points.length; ++i) {
      if (value < points[i]) {
        return labels[i];
      }
    }
  }
  return labels[points.length];
}
