{
  SchedulerService service=null;
  try {
    service=Context.getSchedulerService();
  }
 catch (  Throwable t) {
  }
  Context.addProxyPrivilege(OpenmrsConstants.PRIV_MANAGE_SCHEDULER);
  if (service != null) {
    service.shutdown();
  }
  Context.removeProxyPrivilege(OpenmrsConstants.PRIV_MANAGE_SCHEDULER);
}
