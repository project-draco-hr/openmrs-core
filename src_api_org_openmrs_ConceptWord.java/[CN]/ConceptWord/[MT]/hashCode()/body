{
  int hash=3;
  if (concept != null)   hash=37 * hash + this.getConcept().hashCode();
  if (word != null)   hash=37 * hash + this.getWord().toLowerCase().hashCode();
  if (locale != null)   hash=37 * hash + this.getLocale().hashCode();
  if (synonym != null)   hash=37 * hash + this.getSynonym().hashCode();
  return hash;
}
