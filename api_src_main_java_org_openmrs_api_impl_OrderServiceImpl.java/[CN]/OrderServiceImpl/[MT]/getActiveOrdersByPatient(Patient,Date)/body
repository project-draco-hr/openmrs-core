{
  if (p == null) {
    throw new IllegalArgumentException("patient is required");
  }
  if (date == null) {
    date=new Date();
  }
  List<Patient> patients=new Vector<Patient>();
  patients.add(p);
  return Context.getOrderService().getOrders(Order.class,patients,null,null,null,date,null,Arrays.asList(OrderAction.DISCONTINUE));
}
