{
  if (patient == null) {
    throw new IllegalArgumentException("Patient is required when fetching active orders");
  }
  if (asOfDate == null) {
    asOfDate=new Date();
  }
  if (orderClass == null) {
    orderClass=(Class<Ord>)Order.class;
  }
  return dao.getActiveOrders(patient,orderClass,careSetting,asOfDate);
}
