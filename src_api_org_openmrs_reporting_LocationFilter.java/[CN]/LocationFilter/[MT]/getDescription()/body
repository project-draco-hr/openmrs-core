{
  StringBuilder sb=new StringBuilder();
  if (method.equals(Method.PATIENT_TABLE))   sb.append("Health center");
 else   if (method.equals(Method.LAST_ENCOUNTER))   sb.append("Last encounter at");
 else   if (method.equals(Method.ANY_ENCOUNTER))   sb.append("Any encounter at");
  sb.append(": ").append(location);
  return sb.toString();
}
