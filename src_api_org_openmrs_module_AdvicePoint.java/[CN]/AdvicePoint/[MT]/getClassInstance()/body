{
  if (classInstance != null)   return classInstance;
  Object o=null;
  try {
    Class c=ModuleFactory.getModuleClassLoader(getModule()).loadClass(getClassName());
    o=c.newInstance();
  }
 catch (  Exception e) {
    log.warn("Could not get instance for advice point: " + point,e);
  }
  classInstance=o;
  return o;
}
