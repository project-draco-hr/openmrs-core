{
  if (getIdentifiers() != null && identifiers.size() > 0) {
    PatientIdentifier found=null;
    for (    PatientIdentifier id : identifiers) {
      if (id.getIdentifierType().getName().equals(identifierTypeName)) {
        found=id;
        if (found.isPreferred())         return found;
      }
    }
    return found;
  }
 else {
    return null;
  }
}
