{
  Date ret=null;
  if (withinLastDays != null || withinLastMonths != null) {
    Calendar gc=new GregorianCalendar();
    if (withinLastDays != null)     gc.add(Calendar.DAY_OF_MONTH,-withinLastDays);
    if (withinLastMonths != null)     gc.add(Calendar.MONTH,-withinLastMonths);
    ret=gc.getTime();
  }
  if (sinceDate != null && (ret == null || sinceDate.after(ret)))   ret=sinceDate;
  return ret;
}
