{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  SettingsProperty other=(SettingsProperty)obj;
  if (globalProperty.getProperty() == null) {
    if (other.globalProperty.getProperty() != null)     return false;
  }
 else   if (!globalProperty.getProperty().equals(other.globalProperty.getProperty()))   return false;
  return true;
}
