{
  if (nameSearch != null)   nameSearch=nameSearch.replace(", "," ");
  if (roles == null)   roles=new Vector<Role>();
  Role auth_role=getRole(OpenmrsConstants.AUTHENTICATED_ROLE);
  if (roles.contains(auth_role))   return dao.getUsers(nameSearch,new Vector<Role>(),includeVoided);
 else   return dao.getUsers(nameSearch,roles,includeVoided);
}
