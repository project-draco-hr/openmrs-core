{
  durationType=type.trim();
  durationVal=val.trim();
  op=op.trim();
  if (op.toUpperCase().startsWith("MONTH")) {
    durationOp="months";
  }
 else   if (op.toUpperCase().startsWith("YEAR")) {
    durationOp="years";
  }
 else   if (op.toUpperCase().startsWith("DAY")) {
    durationOp="days";
  }
}
