{
  boolean retVal=false;
  if (isEvaluated) {
    retVal=evaluated;
  }
 else   if (hasConclude) {
    retVal=concludeVal;
  }
 else   if (compOp != null) {
switch (compOp) {
case EQUALS:
{
switch (compOpType) {
case 3:
          retVal=evaluateEquals(answerBool);
        break;
case 2:
      retVal=evaluateEquals(answerInt);
    break;
case 1:
  retVal=evaluateEquals(answerStr);
break;
}
}
break;
case GTE:
{
switch (compOpType) {
case 3:
retVal=evaluateEquals(answerBool);
break;
case 2:
retVal=evaluateGTE(answerInt);
break;
case 1:
retVal=evaluateEquals(answerStr);
break;
}
}
break;
default :
break;
}
}
return retVal;
}
