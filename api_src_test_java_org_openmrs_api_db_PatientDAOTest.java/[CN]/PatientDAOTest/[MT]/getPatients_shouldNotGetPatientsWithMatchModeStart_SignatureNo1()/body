{
  String oldPropertyValue=patientSearchTestHelper.setGlobalProperty(OpenmrsConstants.GLOBAL_PROPERTY_PATIENT_SEARCH_MATCH_MODE,OpenmrsConstants.GLOBAL_PROPERTY_PATIENT_SEARCH_MATCH_START);
  List<Patient> patients=dao.getPatients("xyz",null,null,false,0,11,false);
