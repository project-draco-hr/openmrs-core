{
  Set<Person> people=new HashSet<Person>();
  if (roles == null) {
    people.addAll(Context.getPersonService().getPeople(searchPhrase,includeVoided));
  }
 else {
    for (    User u : Context.getUserService().findUsers(searchPhrase,roles,includeVoided))     people.add(u.getPerson());
  }
  return people;
}
