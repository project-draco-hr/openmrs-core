{
  if (Hl7InArchivesMigrateThread.getTransferStatus() != Status.NONE)   return new Object[]{false,Context.getMessageSourceService().getMessage("Hl7InArchive.migrate.already.running")};
  try {
    if (Context.getHL7Service().startHl7ArchiveMigration())     return new Object[]{true};
    return new Object[]{false,Context.getMessageSourceService().getMessage("Hl7InArchive.migrate.start.fail")};
  }
 catch (  APIAuthenticationException e) {
    return new Object[]{false,Context.getMessageSourceService().getMessage("Hl7InArchive.migrate.authentication.fail")};
  }
}
