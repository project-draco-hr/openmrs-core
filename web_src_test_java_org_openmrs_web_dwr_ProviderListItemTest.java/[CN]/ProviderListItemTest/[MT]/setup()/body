{
  provider=new Provider(){
{
      setPerson(new Person(){
{
          setNames(new HashSet<PersonName>(){
{
              add(new PersonName(){
{
                  setGivenName("givenName");
                  setFamilyName("familyName");
                  setMiddleName("middleName");
                }
              }
);
            }
          }
);
        }
      }
);
    }
  }
;
}
