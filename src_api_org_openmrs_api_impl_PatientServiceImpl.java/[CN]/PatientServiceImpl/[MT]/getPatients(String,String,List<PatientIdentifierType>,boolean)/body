{
  if (name != null && (name.contains("%") || name.contains("*")))   throw new APIException(Context.getMessageSourceService().getMessage("SearchResults.noWildcardsAllowed"));
  if (identifier != null && (identifier.contains("%") || identifier.contains("*")))   throw new APIException(Context.getMessageSourceService().getMessage("SearchResults.noWildcardsAllowed"));
  if (identifierTypes == null)   identifierTypes=Collections.emptyList();
  return dao.getPatients(name,identifier,identifierTypes,matchIdentifierExactly);
}
