{
  String name="";
  if (!StringUtils.isBlank(givenName)) {
    name=givenName;
  }
  if (!StringUtils.isBlank(middleName)) {
    name=name + (name.length() > 0 ? " " : "") + middleName;
  }
  if (!StringUtils.isBlank(familyName)) {
    name=name + (name.length() > 0 ? " " : "") + familyName;
  }
  return name;
}
