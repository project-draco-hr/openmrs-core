{
  if (obs.isVoided()) {
    return obs;
  }
  User user=Context.getAuthenticatedUser();
  Date date=new Date();
  Set<Obs> obsToVoid=new HashSet<Obs>();
  obsToVoid.add(obs);
  fetchObsMembers(obs,obsToVoid);
  for (  Obs o : obsToVoid) {
    if (o.isVoided() == false) {
      o.setVoided(true);
      o.setVoidReason(reason);
      o.setVoidedBy(user);
      o.setDateVoided(date);
    }
  }
  return dao.saveObs(obs);
}
