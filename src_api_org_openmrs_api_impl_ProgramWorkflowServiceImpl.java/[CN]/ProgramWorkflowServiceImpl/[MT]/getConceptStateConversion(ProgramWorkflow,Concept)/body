{
  log.debug("In getcsc with workflow: " + workflow + ", and trigger concept: "+ trigger);
  ConceptStateConversion ret=null;
  if (!Context.getUserContext().hasPrivilege(OpenmrsConstants.PRIV_VIEW_PROGRAMS))   throw new APIAuthenticationException("Privilege required: " + OpenmrsConstants.PRIV_VIEW_PROGRAMS);
  ret=getProgramWorkflowDAO().getConceptStateConversion(workflow,trigger);
  return ret;
}
