{
  try {
    StringWriter w=new StringWriter();
    Velocity.evaluate(velocityContext,w,this.getClass().getName(),field.getDefaultValue());
    xml.append(w.toString());
  }
 catch (  Exception e) {
    log.warn("Error evaluating default value for " + field.getName() + "["+ field.getFieldId()+ "]",e);
  }
}
