{
  Hashtable<Integer,Vector<Obs>> obsMap=new Hashtable<Integer,Vector<Obs>>();
  for (  Obs obs : encounter.getObs()) {
    Vector<Obs> entry=obsMap.get(obs.getObsId());
    if (entry == null)     entry=new Vector<Obs>();
    entry.add(obs);
    obsMap.put(obs.getObsId(),entry);
  }
  return obsMap;
}
