{
  String query=createQuery(phrase,locales,includeRetired,requireClasses,excludeClasses,requireDatatypes,excludeDatatypes,answersToConcept);
  ListPart<ConceptName> names=LuceneQuery.newQuery(query,sessionFactory.getCurrentSession(),ConceptName.class).skipSame("concept.conceptId").listPart(start,size);
  List<ConceptSearchResult> results=Lists.transform(names.getList(),new Function<ConceptName,ConceptSearchResult>(){
    @Override public ConceptSearchResult apply(    ConceptName conceptName){
      return new ConceptSearchResult(phrase,conceptName.getConcept(),conceptName);
    }
  }
);
  return results;
}
