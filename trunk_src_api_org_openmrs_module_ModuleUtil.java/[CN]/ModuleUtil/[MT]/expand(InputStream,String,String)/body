{
  if (log.isDebugEnabled())   log.debug("expanding: " + name);
  File file=new File(fileDir,name);
  FileOutputStream outStream=null;
  try {
    outStream=new FileOutputStream(file);
    OpenmrsUtil.copyFile(input,outStream);
  }
  finally {
    try {
      outStream.close();
    }
 catch (    Exception e) {
    }
  }
  return file;
}
