{
  if ("".equals(personId))   return "?addName=" + name + "&addBirthdate="+ birthdate+ "&addAge="+ age+ "&addGender="+ gender;
 else {
    if ("patient".equals(personType))     return "?patientId=" + personId;
 else     if ("user".equals(personType))     return "?userId=" + personId;
  }
  return "";
}
