{
  String prefix=Action.DISCONTINUE == getAction() ? "DC " : "";
  return prefix + "DrugOrder(" + getDose()+ getDoseUnits()+ " of "+ (getDrug() != null ? getDrug().getName() : "[no drug]")+ " from "+ getDateActivated()+ " to "+ (isDiscontinuedRightNow() ? getDateStopped() : getAutoExpireDate())+ ")";
}
