{
  Boolean found=false;
  String uuid=null;
  for (  CX identifier : identifiers) {
    if (OpenmrsUtil.nullSafeEquals(identifier.getAssigningAuthority().getNamespaceID().getValue(),"UUID")) {
      if (found && !OpenmrsUtil.nullSafeEquals(identifier.getIDNumber().getValue(),uuid))       throw new HL7Exception("multiple UUID values found");
      uuid=identifier.getIDNumber().getValue();
      found=true;
    }
  }
  return uuid;
}
