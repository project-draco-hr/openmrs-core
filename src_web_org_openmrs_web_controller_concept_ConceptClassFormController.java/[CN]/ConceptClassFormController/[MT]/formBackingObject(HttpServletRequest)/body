{
  ConceptClass conceptClass=null;
  if (Context.isAuthenticated()) {
    ConceptService cs=Context.getConceptService();
    String conceptClassId=request.getParameter("conceptClassId");
    if (conceptClassId != null)     conceptClass=cs.getConceptClass(Integer.valueOf(conceptClassId));
  }
  if (conceptClass == null)   conceptClass=new ConceptClass();
  return conceptClass;
}
