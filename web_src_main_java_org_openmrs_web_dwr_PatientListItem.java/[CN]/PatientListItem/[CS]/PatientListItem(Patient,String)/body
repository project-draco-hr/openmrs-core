{
  super(patient,searchName);
  if (patient != null) {
    patientId=patient.getPatientId();
    boolean first=true;
    for (    PatientIdentifier pi : patient.getIdentifiers()) {
      if (first) {
        identifier=pi.getIdentifier();
        first=false;
      }
 else {
        if (!"".equals(otherIdentifiers)) {
          otherIdentifiers+=",";
        }
        otherIdentifiers+=" " + pi.getIdentifier();
      }
    }
  }
}
