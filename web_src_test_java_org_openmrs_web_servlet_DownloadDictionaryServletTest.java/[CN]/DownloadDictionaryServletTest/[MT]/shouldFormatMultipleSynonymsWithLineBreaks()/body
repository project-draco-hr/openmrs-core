{
  String actualContent=runServletWithConcepts(conceptDAO.getConcept(792));
  String expectedStart=EXPECTED_HEADER + "792,\"STAVUDINE LAMIVUDINE AND NEVIRAPINE\",\"Combination antiretroviral drug.\",";
  String expectedEnd=",\"\",\"\",\"Drug\",\"N/A\",\"Super User\",\"Super User\"\n";
  String[] expectedLineBreakSections={"STAVUDINE LAMIVUDINE AND NEVIRAPINE","D4T+3TC+NVP","TRIOMUNE-30","D4T+3TC+NVP"};
  assertContent(expectedStart,expectedEnd,expectedLineBreakSections,actualContent);
}
