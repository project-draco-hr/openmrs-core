{
  Boolean found=false;
  String uuid=null;
  for (  CX identifier : identifiers) {
    if (identifier.getAssigningAuthority().getNamespaceID().getValue().equals("UUID")) {
      if (found && !identifier.getIDNumber().getValue().equals(uuid))       throw new HL7Exception("multiple UUID values found");
      uuid=identifier.getIDNumber().getValue();
      found=true;
    }
  }
  return uuid;
}
