{
  if (obj instanceof PersonName) {
    PersonName pname=(PersonName)obj;
    if (this.personNameId != null && pname.getPersonNameId() != null)     return (this.personNameId.equals(pname.getPersonNameId()));
 else {
      return (OpenmrsUtil.nullSafeEquals(getPerson(),pname.getPerson()) && OpenmrsUtil.nullSafeEquals(getGivenName(),pname.getGivenName()) && OpenmrsUtil.nullSafeEquals(getMiddleName(),pname.getMiddleName())&& OpenmrsUtil.nullSafeEquals(getFamilyName(),pname.getFamilyName()));
    }
  }
  return false;
}
