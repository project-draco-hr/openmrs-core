{
  if (obj instanceof PersonName) {
    PersonName pname=(PersonName)obj;
    if (this.personNameId != null && pname.getPersonNameId() != null)     return (this.personNameId.equals(pname.getPersonNameId()));
 else {
      return (OpenmrsUtil.nullSafeEquals(getPerson(),pname.getPerson()) && OpenmrsUtil.nullSafeEqualsIgnoreCase(getGivenName(),pname.getGivenName()) && OpenmrsUtil.nullSafeEqualsIgnoreCase(getMiddleName(),pname.getMiddleName())&& OpenmrsUtil.nullSafeEqualsIgnoreCase(getFamilyName(),pname.getFamilyName()));
    }
  }
  return false;
}
