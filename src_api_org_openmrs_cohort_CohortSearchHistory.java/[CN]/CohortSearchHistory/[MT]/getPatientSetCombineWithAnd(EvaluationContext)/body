{
  Set<Integer> current=null;
  for (int i=0; i < searchHistory.size(); ++i) {
    Cohort ps=getPatientSet(i,context);
    if (current == null)     current=new HashSet<Integer>(ps.getMemberIds());
 else     current.retainAll(ps.getMemberIds());
  }
  if (current == null)   return Context.getPatientSetService().getAllPatients();
 else {
    return new Cohort(current);
  }
}
