{
  if (isSingleResult()) {
    if (valueDatetime != null)     return valueDatetime;
    if (datatype == Datatype.TEXT && valueText != null) {
      try {
        return OpenmrsUtil.getDateFormat().parse(valueText);
      }
 catch (      Exception e) {
      }
    }
    return valueDatetime;
  }
  return this.get(0).toDatetime();
}
