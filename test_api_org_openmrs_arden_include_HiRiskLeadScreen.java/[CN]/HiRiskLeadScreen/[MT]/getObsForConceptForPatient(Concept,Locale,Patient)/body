{
  List<Obs> MyObs;
  Obs obs=new Obs();
{
    MyObs=Context.getObsService().getObservationsByPersonAndConcept(patient,concept);
    Iterator iter=MyObs.iterator();
    if (iter.hasNext()) {
      while (iter.hasNext()) {
        obs=(Obs)iter.next();
      }
      return obs;
    }
 else {
      return null;
    }
  }
}
