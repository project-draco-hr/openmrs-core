{
  for (  ModuleClassLoader classLoader : ModuleFactory.getModuleClassLoaders()) {
    try {
      Class<?> c=classLoader.loadClass(name);
      loadedClasses.add(c);
      return c;
    }
 catch (    ClassNotFoundException e) {
    }
  }
  Class<?> c=getParent().loadClass(name);
  loadedClasses.add(c);
  return c;
}
