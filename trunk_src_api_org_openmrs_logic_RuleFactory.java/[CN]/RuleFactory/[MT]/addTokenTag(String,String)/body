{
  if (!tagsByToken.containsKey(token))   tagsByToken.put(token,new HashSet<String>());
  if (!tagsByToken.get(token).contains(tag))   tagsByToken.get(token).add(tag);
  if (!tokensByTag.containsKey(tag))   tokensByTag.put(tag,new HashSet<String>());
  if (!tokensByTag.get(tag).contains(token))   tokensByTag.get(tag).add(token);
}
