{
  if (getAddresses() != null && getAddresses().size() > 0) {
    for (    PersonAddress addr : getAddresses()) {
      if (addr.isPreferred() && !addr.isVoided()) {
        return addr;
      }
    }
    for (    PersonAddress addr : getAddresses()) {
      if (!addr.isVoided()) {
        return addr;
      }
    }
    if (isVoided() && !getAddresses().isEmpty()) {
      return getAddresses().iterator().next();
    }
  }
  return null;
}
