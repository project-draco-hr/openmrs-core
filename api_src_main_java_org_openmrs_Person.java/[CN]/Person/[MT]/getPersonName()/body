{
  if (getNames() != null && getNames().size() > 0) {
    for (    PersonName name : getNames()) {
      if (name.isPreferred() && !name.isVoided()) {
        return name;
      }
    }
    for (    PersonName name : getNames()) {
      if (!name.isVoided()) {
        return name;
      }
    }
    if (isVoided() && !getNames().isEmpty()) {
      return getNames().iterator().next();
    }
  }
  return null;
}
