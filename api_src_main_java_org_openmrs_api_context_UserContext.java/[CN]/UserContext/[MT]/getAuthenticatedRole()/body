{
  if (authenticatedRole != null)   return authenticatedRole;
  authenticatedRole=Context.getUserService().getRole(OpenmrsConstants.AUTHENTICATED_ROLE);
  if (authenticatedRole == null) {
    throw new RuntimeException("Database out of sync with code: " + OpenmrsConstants.AUTHENTICATED_ROLE + " role does not exist");
  }
  return authenticatedRole;
}
