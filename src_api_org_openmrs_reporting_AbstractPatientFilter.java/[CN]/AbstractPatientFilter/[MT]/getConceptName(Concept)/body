{
  if (concept == null)   return "[CONCEPT]";
  String cName="";
  try {
    ConceptName conceptName=concept.getName();
    if (conceptName == null)     conceptName=Context.getConceptService().getConcept(concept.getConceptId()).getName();
    cName=conceptName.getName();
  }
 catch (  Exception e) {
    cName="";
  }
  return cName;
}
