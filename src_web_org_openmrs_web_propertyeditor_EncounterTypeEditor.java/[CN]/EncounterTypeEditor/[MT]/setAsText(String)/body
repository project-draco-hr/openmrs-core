{
  if (context != null) {
    EncounterService ps=context.getEncounterService();
    if (StringUtils.hasText(text)) {
      try {
        setValue(ps.getEncounterType(Integer.valueOf(text)));
      }
 catch (      Exception ex) {
        throw new IllegalArgumentException("Encounter Type not found: " + ex.getMessage());
      }
    }
 else {
      setValue(null);
    }
  }
}
