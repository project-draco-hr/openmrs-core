{
  Properties props=new Properties();
  java.net.URL url=OpenmrsConstants.class.getClassLoader().getResource("openmrs.properties");
  if (url == null) {
    log.error("Unable to find openmrs.properties file built by maven");
    return null;
  }
  try {
    props.load(url.openStream());
    return props.getProperty("openmrs.version");
  }
 catch (  IOException e) {
    log.error("Unable to get pom.properties file into Properties object");
  }
  return null;
}
