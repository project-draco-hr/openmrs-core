{
  File file=null;
  String pathName=OpenmrsUtil.getRuntimePropertiesFilePathName(WebConstants.WEBAPP_NAME);
  if (pathName != null) {
    file=new File(pathName);
  }
 else   file=new File(OpenmrsUtil.getApplicationDataDirectory(),getRuntimePropertiesFileName());
  log.debug("Using file: " + file.getAbsolutePath());
  return file;
}
