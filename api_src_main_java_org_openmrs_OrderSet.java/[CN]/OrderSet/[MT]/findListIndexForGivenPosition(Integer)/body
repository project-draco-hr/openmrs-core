{
  Integer size=getOrderSetMembers().size();
  if (position != null) {
    if (position < 0 && position >= (-1 - size)) {
      position=position + size + 1;
    }
 else     if (position > size) {
      throw new APIException("Cannot add a member which is out of range of the list");
    }
  }
 else {
    position=size;
  }
  return position;
}
