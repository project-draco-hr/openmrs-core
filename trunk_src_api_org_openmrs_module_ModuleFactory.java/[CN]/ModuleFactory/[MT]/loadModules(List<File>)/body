{
  for (  File f : modulesToLoad) {
    if (!f.getName().startsWith(".")) {
      try {
        Module mod=loadModule(f);
        log.debug("Loaded module: " + mod + " successfully");
      }
 catch (      Throwable t) {
        log.debug("Unable to load file in module directory: " + f + ". Skipping file.",t);
      }
    }
  }
}
