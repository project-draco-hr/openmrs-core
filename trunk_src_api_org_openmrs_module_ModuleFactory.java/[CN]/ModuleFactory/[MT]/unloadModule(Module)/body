{
  if (isModuleStarted(mod))   stopModule(mod,true);
  getLoadedModules().remove(mod);
  if (mod != null) {
    System.gc();
    System.gc();
    File file=mod.getFile();
    boolean deleted=file.delete();
    if (!deleted) {
      file.deleteOnExit();
      log.warn("Could not delete " + file.getAbsolutePath());
    }
    file=null;
    mod=null;
  }
}
