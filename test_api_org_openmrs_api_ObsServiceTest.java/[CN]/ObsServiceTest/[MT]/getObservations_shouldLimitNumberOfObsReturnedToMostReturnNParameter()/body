{
  executeDataSet(INITIAL_OBS_XML);
  ObsService obsService=Context.getObsService();
  List<Obs> obss=obsService.getObservations(Collections.singletonList(new Person(8)),null,null,null,null,null,null,1,null,null,null,false);
  Assert.assertEquals(1,obss.size());
  Assert.assertEquals(8,obss.get(0).getObsId().intValue());
}
