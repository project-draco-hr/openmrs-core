{
  if (isVoided())   return false;
  if (checkDate == null) {
    checkDate=new Date();
  }
  if (dateActivated != null && checkDate.before(dateActivated)) {
    return false;
  }
  if (isDiscontinuedRightNow()) {
    if (dateStopped == null)     return checkDate.equals(dateActivated);
 else     return checkDate.before(dateStopped);
  }
 else {
    if (autoExpireDate == null)     return true;
 else     return checkDate.before(autoExpireDate);
  }
}
