{
  User user=(User)obj;
  if (user == null) {
    errors.rejectValue("user","error.general");
  }
 else {
    if (user.isVoided() && !StringUtils.hasText(user.getVoidReason()))     errors.rejectValue("voidReason","error.null");
  }
  if (!isUserNameValid(user.getUsername()))   errors.rejectValue("username","error.username.pattern");
}
