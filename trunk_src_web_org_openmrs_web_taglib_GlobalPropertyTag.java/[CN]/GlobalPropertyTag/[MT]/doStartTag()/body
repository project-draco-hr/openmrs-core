{
  Object value;
  if (StringUtils.hasText(listSeparator))   value=Collections.singletonList(defaultValue);
 else   value=defaultValue;
  if (Context.isAuthenticated()) {
    if (StringUtils.hasText(listSeparator)) {
      String stringVal=(String)Context.getAdministrationService().getGlobalProperty(key,defaultValue);
      value=Arrays.asList(stringVal.split(listSeparator));
    }
 else     value=(String)Context.getAdministrationService().getGlobalProperty(key,defaultValue);
  }
  try {
    if (var != null)     pageContext.setAttribute(var,value);
 else     pageContext.getOut().write(value.toString());
  }
 catch (  Exception e) {
    log.error("error getting global property",e);
  }
  return SKIP_BODY;
}
