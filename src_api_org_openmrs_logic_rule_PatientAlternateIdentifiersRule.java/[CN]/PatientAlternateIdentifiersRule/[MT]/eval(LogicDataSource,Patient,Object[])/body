{
  List<PatientIdentifier> identifierList=patient.getActiveIdentifiers();
  if (identifierList.size() > 1) {
    Result result=null;
    for (int i=1; i < identifierList.size(); i++) {
      if (result == null)       result=new Result(identifierList.get(i).getIdentifier());
 else       result.add(new Result(identifierList.get(i).getIdentifier()));
    }
    return result;
  }
 else {
    return Result.NULL_RESULT;
  }
}
