{
  List<String> subList=ConceptWord.getUniqueWords(matchedString);
  double size=ConceptWord.splitPhrase(matchedString).length;
  double matches=0.0;
  for (  String s : subList) {
    s=s.toUpperCase();
    for (    String p : searchedWords) {
      p=p.toUpperCase();
      if (p.startsWith(s))       matches+=1.0;
    }
  }
  return matches == 0 ? 0 : (matches / size);
}
