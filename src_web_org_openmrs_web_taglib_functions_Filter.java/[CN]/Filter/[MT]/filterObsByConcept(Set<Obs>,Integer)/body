{
  log.debug("Filtering obs for concept id: " + concept);
  Set<Obs> ret=new HashSet<Obs>();
  if (obs != null) {
    for (Iterator<Obs> i=obs.iterator(); i.hasNext(); ) {
      Obs o=i.next();
      if (o.getConcept().getConceptId().intValue() == concept.intValue()) {
        ret.add(o);
      }
    }
  }
  return ret;
}
