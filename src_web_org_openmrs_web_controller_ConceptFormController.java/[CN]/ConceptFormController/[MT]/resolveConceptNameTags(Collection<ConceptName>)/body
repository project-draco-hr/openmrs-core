{
  ConceptService cs=Context.getConceptService();
  for (  ConceptName cn : conceptNames) {
    if (cn.getTags() != null) {
      for (      ConceptNameTag tag : cn.getTags()) {
        ConceptNameTag existingTag=cs.getConceptNameTagByName(tag.getTag());
        if (existingTag == null) {
        }
 else {
          cn.removeTag(tag);
          cn.addTag(existingTag);
        }
      }
    }
  }
}
