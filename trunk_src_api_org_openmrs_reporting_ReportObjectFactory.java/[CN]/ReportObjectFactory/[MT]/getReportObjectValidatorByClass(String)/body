{
  if (modules != null && currentClassName != null) {
    String validator="";
    for (int i=0; i < modules.size(); i++) {
      ReportObjectFactoryModule mod=modules.get(i);
      if (currentClassName.equals(mod.getClassName())) {
        validator=mod.getValidatorClass();
      }
    }
    return validator;
  }
 else {
    return "";
  }
}
