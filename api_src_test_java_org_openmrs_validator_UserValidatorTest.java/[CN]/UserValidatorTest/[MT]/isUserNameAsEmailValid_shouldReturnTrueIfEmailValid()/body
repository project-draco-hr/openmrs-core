{
  String[] valids=new String[]{"mkyong@yahoo.com","mkyong-100@yahoo.com","mkyong.100@yahoo.com","mkyong111@mkyong.com","mkyong-100@mkyong.net","mkyong.100@mkyong.com.au","mkyong@1.com","mkyong@gmail.com.com","mk@t-yong.de"};
  for (  String email : valids) {
    Assert.assertTrue(validator.isUserNameAsEmailValid(email));
  }
}
