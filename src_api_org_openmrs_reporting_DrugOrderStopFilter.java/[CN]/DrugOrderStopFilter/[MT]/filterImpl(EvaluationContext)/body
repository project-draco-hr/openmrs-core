{
  return Context.getPatientSetService().getPatientsHavingDrugOrder(getDrugList(),getGenericDrugList(),null,null,OpenmrsUtil.fromDateHelper(null,withinLastDays,withinLastMonths,untilDaysAgo,untilMonthsAgo,sinceDate,untilDate),OpenmrsUtil.toDateHelper(null,withinLastDays,withinLastMonths,untilDaysAgo,untilMonthsAgo,sinceDate,untilDate),discontinued,getDiscontinuedReasonList());
}
