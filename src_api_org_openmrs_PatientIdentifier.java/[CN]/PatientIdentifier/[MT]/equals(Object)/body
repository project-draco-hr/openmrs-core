{
  if (obj instanceof PatientIdentifier) {
    PatientIdentifier p=(PatientIdentifier)obj;
    boolean ret=(patient != null && patient.equals(p.getPatient())) && (identifier != null && identifier.equals(p.getIdentifier())) && (identifierType != null && identifierType.equals(p.getIdentifierType()));
    return ret || p == this;
  }
  return false;
}
