{
  if (obj instanceof PatientIdentifier) {
    PatientIdentifier p=(PatientIdentifier)obj;
    boolean ret=true;
    if (patient != null && p.getPatient() != null)     ret=ret && patient.equals(p.getPatient());
    if (identifier != null && p.getIdentifier() != null)     ret=ret && identifier.equals(p.getIdentifier());
    if (identifierType != null && p.getIdentifierType() != null)     ret=ret && identifierType.equals(p.getIdentifierType());
    return ret;
  }
  return false;
}
