{
  Class<?> clas=null;
  try {
    clas=super.findLoadedClass(name);
  }
 catch (  Exception e) {
  }
  if (clas == null) {
    try {
      clas=super.findSystemClass(name);
    }
 catch (    Exception e) {
    }
  }
  if (clas == null) {
    try {
      clas=OpenmrsClassLoader.getInstance().loadClass(name);
    }
 catch (    Exception e) {
    }
  }
  return clas;
}
