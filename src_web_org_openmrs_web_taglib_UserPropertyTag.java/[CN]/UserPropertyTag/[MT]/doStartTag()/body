{
  String value=defaultValue;
  if (Context.isAuthenticated()) {
    String prop=Context.getAuthenticatedUser().getUserProperty(key);
    if (prop != null && !prop.equals(""))     value=prop;
  }
  try {
    if (var != null)     pageContext.setAttribute(var,value);
 else     pageContext.getOut().write(value);
  }
 catch (  Exception e) {
    log.error("error getting global property",e);
  }
  return SKIP_BODY;
}
