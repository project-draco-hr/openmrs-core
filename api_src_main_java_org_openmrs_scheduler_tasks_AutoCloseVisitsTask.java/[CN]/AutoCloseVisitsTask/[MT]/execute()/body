{
  if (!isExecuting) {
    if (log.isDebugEnabled())     log.debug("Starting Auto Close Visits Task...");
    startExecuting();
    try {
      Context.getVisitService().stopVisits(new Date());
    }
 catch (    Exception e) {
      log.error("Error while auto closing visits:",e);
    }
 finally {
      stopExecuting();
    }
  }
}
