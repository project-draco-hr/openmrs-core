{
  if (t == null)   throw new ModuleException("Startup error value cannot be null",this.getModuleId());
  StringBuffer sb=new StringBuffer();
  if (exceptionMessage != null) {
    sb.append(exceptionMessage);
    sb.append("\n");
  }
  sb.append(t.getMessage());
  sb.append("\n");
  for (  StackTraceElement traceElement : t.getStackTrace()) {
    if (traceElement.getClassName().contains("openmrs"))     sb.append(" ** ");
    sb.append(traceElement);
    sb.append("\n");
  }
  this.startupErrorMessage=sb.toString();
}
