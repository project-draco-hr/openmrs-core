{
  int index=Arrays.asList(propertyNames).indexOf(propertyToSet);
  if (value == null) {
    return false;
  }
  if (index >= 0) {
    if (currentState[index] == null || !setNullOnly) {
      if (!value.equals(currentState[index])) {
        currentState[index]=value;
        return true;
      }
    }
  }
  return false;
}
