{
  if (!Context.hasPrivilege(OpenmrsConstants.PRIV_VIEW_ENCOUNTERS))   throw new APIAuthenticationException("Privilege required: " + OpenmrsConstants.PRIV_VIEW_ENCOUNTERS);
  if (log.isDebugEnabled())   log.debug("Getting encounter with id: " + encounterId);
  return getEncounterDAO().getEncounter(encounterId);
}
